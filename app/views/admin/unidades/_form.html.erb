<%= simple_form_for [:admin, @unidade] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :sigla %>
    <%= f.input :fone %>
    <%= f.input :cnpj %>
    <%= f.input :ramal %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
